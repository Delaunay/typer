


%   Constructor
% -------------------------------------------

% only inductive-cons is available

inductive-cons Nat succ;

% Usage
zero = inductive-cons Nat zero; % those are aliases
succ = inductive-cons Nat succ;

one = (succ (zero));

% -------------------------------------------
%   Cases
% -------------------------------------------

case x
    | zero => y
    | succ x => succ (plus x y);
